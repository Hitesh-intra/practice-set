public class exeption {
    public static void main(String[] args) {

        int a=10;
        int b=0;
        int c=0;
        try{
            c=a/b;

        }
        catch (Exception e){
            System.out.println(c);
            System.out.println( e);
        }
        finally {
            System.out.println("sorry we cnat divide by zero");
        }
    }
}
