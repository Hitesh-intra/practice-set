public class facto {

    public static void main(String[] args) {



      facto(5);
    }
    public static  void facto(int facto){
        int i=facto-1;

        while(i>0){ //this i will go from facto-1 to 1 for satisfying the condition
            facto*=i;
            i--;}



        System.out.println(facto);

    }
}
